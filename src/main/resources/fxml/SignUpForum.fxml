            <TextField layoutX="79.0" layoutY="313.0" promptText="Your password" />
            <ImageView fitHeight="89.0" fitWidth="85.0" layoutX="111.0" layoutY="71.0" pickOnBounds="true" preserveRatio="true" style="-fx-effect: colorAdjust;">
