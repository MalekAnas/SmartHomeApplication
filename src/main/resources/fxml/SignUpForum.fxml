<AnchorPane fx:id="BK" prefHeight="600.0" prefWidth="300.0" stylesheets="@../styles/SignUpStyle.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.matchit.Controllers.SignUpController">
